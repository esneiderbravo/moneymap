{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "browser": true,
    "jest": true,
    "jquery": true,
    "node": true,
    "es6": true
  },
  "parser": "@babel/eslint-parser",
  "settings": {
    "react": {
      "version": "17.0.2"
    }
  },
  "globals": {
    "componentHandler": "readonly"
  },
  "rules": {
    "no-restricted-imports": [
      "error",
      {
        "patterns": ["@mui/*/*/*"]
      }
    ]
  }
}
